{
    "manifest_version": 2,
    "name": "Highlighted Text Extension",
    "version": "1.0",
    "description": "Displays an alert with the highlighted text.",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}