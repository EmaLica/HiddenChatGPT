{
    "manifest_version": 2,
  "name": "Chrome Plugin",
  "description": "OpenAI Query",
  "version": "1.0",
  "content_scripts":[
      {
        "matches":[
          "http://oracle.com/*"
        ],
        "js": ["content.js"]
      }
    ],
  "browser_action": {
      "default_icon": "icon.png"
    },
  "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
  "permissions": [
      "activeTab",
      "storage"
    ],
    "commands" : {
        "calcola" : {
            "suggested_key" : {
                "default" : "Ctrl+Shift+1"
            },
            "description" :"OpenAI Query"
        }
    }


  }
